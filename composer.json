    "illuminate/config": "^11.25",
    "illuminate/database": "^11.25"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3",
    "mockery/mockery": "^1.6"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ssmiff\\LaravelEventSauce\\ModuleServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
