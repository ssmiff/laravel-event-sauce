{
  "name": "ssmiff/laravel-event-sauce",
  "autoload": {
    "psr-4": {
      "Ssmiff\\LaravelEventSauce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ssmiff\\LaravelEventSauce\\Test\\": "tests/"
    }
